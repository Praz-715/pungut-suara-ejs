                                        <a href="/pemilihan/<%= pemilihanDisini.slug %> ">https://<%= pemilihanDisini.host %>/<%= pemilihanDisini.slug %> </a>
                                    </div>
                                    <hr />
                                </div>
                                <div class="row">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Berlangsung</label>
                                    <div class="col-md-8 col-sm-6=8 col-xs-12">
                                        <%= pemilihanDisini.waktuAwal %>
                                    </div>
                                </div>
                                <div class="row">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Sampai</label>
                                    <div class="col-md-8 col-sm-6=8 col-xs-12">
                                        <%= pemilihanDisini.waktuAkhir %>
                                    </div>
                                </div>
                                <div class="row">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Sifat Pemilihan</label>
                                    <div class="col-md-8 col-sm-6=8 col-xs-12">
                                        <%= (pemilihanDisini.pemilihanTerbuka ? 'Terbuka' : 'Tertutup') %>
                                    </div>
                                </div>
                                <div class="row">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12">
                          Mode Grup
                          <button type="button" class="btn btn-default ml-5" style="border: none; padding: 0px 0px 0px 0px;" onclick="new PNotify({
                            title: 'Mode Grup',
                            text: 'Artinya membuat beberapa grup yang bisa dipilih oleh Pemilih dalam 1 saja pemilihan, contohnya group Pemilihan Presiden dan group Pemilihan DPR',
                            type: 'info',
                            styling: 'bootstrap3'
                        });"><i class="fa fa-exclamation-circle"></i></button>
                        </label>
                                    <div class="col-md-8 col-sm-6=8 col-xs-12">
                                        <%= (pemilihanDisini.modeGroup ? 'Ya' : 'Tidak') %>
                                    </div>
                                </div>
                                <div class="row">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Status Pemilihan</label>
                                    <div class="col-md-8 col-sm-6=8 col-xs-12">
                                        <%= pemilihanDisini.statusPemilihan %>
                                            <div id="countdown"></div>
                                    </div>
                                </div>
                                <% if (pemilihanDisini.deskripsi) { %>
                                    <div class="row">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Deskripsi</label>
                                        <div class="col-md-8 col-sm-6=8 col-xs-12">
                                            <%= pemilihanDisini.deskripsi %>
                                        </div>
                                    </div>
                                    <% } %>
                                        <% if (pemilihanDisini.fotoKampanye) { %>
                                            <div class="row">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12">Foto Lembaga</label>
                                                <div class="col-md-8 col-sm-6=8 col-xs-12">
                                                    <%= pemilihanDisini.fotoKampanye %>
                                                </div>
                                            </div>
                                            <% } %>
                                                <% if (pemilihanDisini.videoKampanye) { %>
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Video Kampanye</label>
                                                        <div class="col-md-8 col-sm-6=8 col-xs-12">
                                                            <%= pemilihanDisini.videoKampanye %>
                                                        </div>
                                                    </div>
                                                    <% } %>
                            </div>
                        </div>
                    </div>

